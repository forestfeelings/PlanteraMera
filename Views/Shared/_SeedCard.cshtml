@model Seed

<div class="col-sm-12 col-md-4">
    <div class="thumbnail">
        <img src="~/images/@(Model.Type).jpg" class="img-fluid" alt="@Model.Name" />
        <div class="caption">
            <h3 class="fa-pull-right">@Model.Price SEK</h3>
            <h3>
                @Model.Name
            </h3>
            <p><strong>@Model.Type</strong></p>
            <button id="buyButton" class="btn btn-success">Köp</button>
        </div>
    </div>
</div>